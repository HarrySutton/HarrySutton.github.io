---
import "../styles/global.css"

interface Props {
	title: string;
	description: string;
	image?: string;
	keywords?: string[];
}

const { title, description, keywords } = Astro.props;
---

<head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <meta name="viewport" content="width=device-width" />
    <title>{title}</title>
</head>